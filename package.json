{
    "name": "yamb"
  , "version": "0.0.2"
  , "author": "Alexey Simonenko <alexey@simonenko.su>"
  , "main": "./lib/index"
  , "repository": {
        "type": "git"
      , "url": "https://github.com/yamb/yamb.git"
    }
  , "engines": {
      "node": "0.11.x"
    }
  , "dependencies": {
        "co": "2.3.x"

      , "marked": "0.2.x"
      , "mdash": "0.0.x"
      , "highlight.js": "7.5.x"
      , "yandex-translate": "1.0.x"
    }
  , "devDependencies": {
        "jshint": "2.3.x"

      , "mocha": "1.15.x"
      , "should": "2.1.x"

      , "coveralls": "2.6.x"
      , "istanbul": "0.1.x"
      , "mocha-istanbul": "0.2.x"
    }
  , "scripts": {
        "lib-cov": "./node_modules/.bin/istanbul --harmony instrument --output lib-cov --no-compact --variable global.__coverage__ lib"
      , "cover": "npm run lib-cov && COVERAGE=1 ISTANBUL_REPORTERS=lcovonly ./node_modules/.bin/mocha --harmony --reporter mocha-istanbul && cat lcov.info | ./node_modules/.bin/coveralls && rm -rf lib-cov lcov.info"

      , "jshint": "./node_modules/.bin/jshint lib"
      , "mocha": "./node_modules/.bin/mocha --harmony test"

      , "test": "npm run jshint && npm run mocha"
    }
}